{
  "name": "lab7",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "start": "node build/index.js",
    "dev": "nodemon -q build/index.js",
    "lint": "eslint .  --ignore-path .eslintignore --ext .ts",
    "lint:fix": "eslint --ignore-path .eslintignore --ext .ts --fix",
    "format": "prettier --ignore-path .prettierignore --check \"**/*.+(ts|json)\"",
    "format:fix": "prettier --ignore-path .prettierignore --write \"**/*.+(ts|json)\"",
    "typeorm": "npm run build && npx typeorm -d build/typeorm.config.js",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "http-status-codes": "^2.2.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "tsc-alias": "^1.8.6",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build && npm start",
    "ext": "ts"
  }
}
