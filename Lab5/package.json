{
  "name": "lab5",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "start": "node dist/index.js",
    "dev": "nodemon -q build/index.js"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "concurrently": "^8.0.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.22",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build && ls && node build/index.js",
    "ext": "ts, js"
  }
}

